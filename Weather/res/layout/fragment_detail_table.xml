<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res/com.yeyu.weather"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <com.yeyu.widget.GraphicTableView
        android:id="@+id/graphic_table"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        custom:axisX="250dp"
        custom:axisY="150dp"
        custom:axisXInternal="30dp"
        custom:axisYInternal="2dp" />
</LinearLayout>
